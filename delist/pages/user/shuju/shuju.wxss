.bj{
  display: flex;
  z-index:-1;
  opacity: 0.8;
  position: absolute;
}
.container-part1{
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
  background-color: rgb(191, 241, 241);
}
.container-section{
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  width: 25%;
  height: auto;
  padding:5px 7px;
}
.container-text{
  margin-top: 5px;
  float: left;
  text-align: center;
  font-size: 1.4em;
  white-space: nowrap;
}
.container-part2{
  font-size: 1.6em;
  padding-top: 12px;
  margin-left: 6px;
  float: left;
}

